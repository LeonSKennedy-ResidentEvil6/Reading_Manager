<h1>Edit reading list <%= @list.title %></h1>

<%= render partial: "layouts/errors", locals: { object: @list } %>

<%= form_for(@list) do |f| %>

    <%= f.label :title %><br>
    <%= f.text_field :title %><br>

    <%= f.label :description %><br>
    <%= f.text_area :description %><br>

    <%= f.submit %>

<% end %>